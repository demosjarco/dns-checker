{
	"type": "module",
	"scripts": {
		"build": "qwik build",
		"build:client": "vite build",
		"build:preview": "vite build --ssr src/entry.preview.tsx",
		"build:server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
		"build:types:cf": "wrangler types",
		"postbuild:types:cf": "prettier --write worker-configuration.d.ts",
		"build:types:tsc": "tsc --noEmit",
		"build:types": "npm run build:types:cf && npm run build:types:tsc",
		"build:db": "drizzle-kit generate --dialect=sqlite --casing=snake_case --schema=\"db/index.ts\" --out=\"db/migrations\"",
		"postbuild:db": "prettier --write db",
		"dev": "vite --mode ssr",
		"dev:debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --check .",
		"fmt:fix": "prettier --write .",
		"lint": "eslint **/*.*ts*",
		"lint:fix": "npm run lint -- --fix",
		"preview": "qwik build preview && vite preview --open",
		"preview:lhci": "qwik build preview && vite preview --port 8080",
		"preclean": "npx -y rimraf@latest --glob ./**/.wrangler ./**/*.tsbuildinfo",
		"clean": "npx -y rimraf@latest ./dist ./server ./tmp",
		"start:clean": "npm run clean && qwik build && npm run start",
		"start:build": "qwik build && npm run start",
		"start": "vite --open --mode ssr",
		"serve": "wrangler dev --test-scheduled --live-reload true",
		"qwik": "qwik",
		"deploy:db": "wrangler d1 migrations apply PROBE_DB --remote"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/demosjarco/dns-checker.git"
	},
	"author": "DemosJarco <1209494+demosjarco@users.noreply.github.com>",
	"license": "MIT",
	"private": true,
	"bugs": {
		"url": "https://github.com/demosjarco/dns-checker/issues"
	},
	"homepage": "https://github.com/demosjarco/dns-checker#readme",
	"engines": {
		"node": "^18.17.0 || ^20.3.0 || >=21.0.0"
	},
	"engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
	"dependencies": {
		"@chainfuse/helpers": "^4.2.9",
		"@chainfuse/types": "^4.2.3",
		"drizzle-orm": "^0.45.1",
		"iata-location": "^1.0.1",
		"leaflet": "^1.9.4",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@builder.io/qwik": "^1.17.2",
		"@builder.io/qwik-city": "^1.17.2",
		"@demosjarco/prettier-config": "^1.0.2",
		"@eslint/js": "^9.39.2",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@tailwindcss/vite": "^4.1.17",
		"@types/leaflet": "^1.9.21",
		"drizzle-kit": "^0.31.8",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-qwik": "^1.18.0",
		"flowbite": "^4.0.1",
		"prettier": "^3.7.4",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"tailwindcss": "^4.1.17",
		"turnstile-types": "^1.2.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.56.0",
		"zaraz-ts": "^1.2.0"
	}
}
